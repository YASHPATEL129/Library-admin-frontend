<div class="admin-body">
  <h2>Admins</h2>
  <table class="table" id="adminTable" border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
        <th>Contact</th>
        <th>Created By</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody id="adminTableBody">
      <!-- Categories will be dynamically populated here -->
    </tbody>
  </table>
  <div
    class="modal fade"
    id="editAdminPasswordModal"
    tabindex="-1"
    role="dialog"
    aria-labelledby="editAdminPasswordModalLabel"
    aria-hidden="true"
  >
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="editAdminPasswordModalLabel">
            Edit Category
          </h5>
        </div>
        <div class="modal-body">
          <div class="form-group">
            <input type="hidden" id="editAdminEmail" value="1" />
            <label for="editPassword">Password</label>
            <input
              type="text"
              class="form-control"
              id="editPassword"
              placeholder="Enter Password"
              autocomplete="off"
            />
          </div>
        </div>
        <div class="modal-footer">
          <button
            type="button"
            class="btn btn-secondary"
            data-bs-dismiss="modal"
            id="edit-close-button"
          >
            Close
          </button>
          <button
            type="button"
            class="btn btn-primary"
            onclick="saveEditedAdminPassword()"
          >
            Save changes
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="js/admin.js"></script>
<script>
  fetchAdminList(false);
</script>
